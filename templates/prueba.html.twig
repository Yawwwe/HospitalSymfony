{% extends 'default.html.twig' %}
{% block contenido %}
<h1>productos</h1>

<table>
    <tr>
	<td>Nombre</td>
	</tr>
	{% for producto in productos %}
        <tr>
		<td>{{ producto.name }}</td>
		<td>{{ producto.price }}</td>
